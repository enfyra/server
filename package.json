{
  "name": "enfyra_be",
  "version": "0.6.6-beta",
  "description": "Dynamic backend platform that auto-generates REST and GraphQL APIs from database schemas",
  "author": "dothinh115 <dothinh115@gmail.com>",
  "private": false,
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "build": "nest build && cp -r data dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "postinstall": "node -e \"if (!require('fs').existsSync('./node_modules/sharp')) { require('child_process').execSync('yarn add sharp --ignore-engines', {stdio: 'inherit'}) }\""
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.23",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.1.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^8.0.7",
    "@types/chalk": "^2.2.4",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "bcryptjs": "^3.0.2",
    "chalk": "latest",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "latest",
    "generic-pool": "^3.9.0",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.13.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.20.0",
    "multer": "latest",
    "mysql2": "^3.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.16.1",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "ts-morph": "^26.0.0",
    "uuid": "latest",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.2",
    "@types/express": "^4.17.17",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^24.3.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.0.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testMatch": [
      "<rootDir>/test/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/**/*.spec.ts",
      "!src/**/*.interface.ts",
      "!src/**/*.dto.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "cli-table3": "0.6.3",
    "@nestjs/cli": "10.3.2"
  }
}
